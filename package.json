{
  "name": "Nyth",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "build:ipa": "chmod +x scripts/build-ipa.sh && ./scripts/build-ipa.sh",
    "build:ipa:debug": "chmod +x scripts/build-ipa.sh && ./scripts/build-ipa.sh Debug",
    "build:ipa:release": "chmod +x scripts/build-ipa.sh && ./scripts/build-ipa.sh Release",
    "cache:status": "./scripts/cache-status.sh",
    "clean": "./scripts/clean-cache.sh --all",
    "clean:android": "./scripts/clean-cache.sh --android",
    "clean:ios": "./scripts/clean-cache.sh --ios",
    "clean:metro": "./scripts/clean-cache.sh --metro",
    "clean:npm": "./scripts/clean-cache.sh --npm",
    "clean:quick": "./scripts/quick-clean.sh",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "test:auth": "jest __tests__/auth --passWithNoTests",
    "prepare": "husky",
    "bundle:android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output ./build/index.android.bundle --assets-dest ./build | cat",
    "bundle:ios": "react-native bundle --platform ios --dev false --entry-file index.js --bundle-output ./build/index.ios.bundle --assets-dest ./build | cat",
    "analyze:android": "npm run bundle:android && npx react-native bundle-visualizer ./build/index.android.bundle --open false | cat",
    "analyze:ios": "npm run bundle:ios && npx react-native bundle-visualizer ./build/index.ios.bundle --open false | cat"
  },
  "dependencies": {
    "@bam.tech/react-native-image-resizer": "^3.0.11",
    "@invertase/react-native-apple-authentication": "^2.4.1",
    "@notifee/react-native": "^9.1.8",
    "@react-native-async-storage/async-storage": "^1.24.0",
    "@react-native-camera-roll/camera-roll": "^7.10.2",
    "@react-native-clipboard/clipboard": "^1.16.3",
    "@react-native-community/blur": "^4.4.1",
    "@react-native-community/datetimepicker": "^8.4.4",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-community/push-notification-ios": "^1.11.0",
    "@react-native-community/slider": "^5.0.1",
    "@react-native-documents/picker": "^10.1.5",
    "@react-native-firebase/analytics": "^23.0.1",
    "@react-native-firebase/app": "^23.0.1",
    "@react-native-firebase/auth": "^23.0.1",
    "@react-native-firebase/firestore": "^23.0.1",
    "@react-native-firebase/functions": "^23.0.1",
    "@react-native-firebase/messaging": "^23.0.1",
    "@react-native-firebase/storage": "^23.0.1",
    "@react-native-google-signin/google-signin": "^15.0.0",
    "@react-native/new-app-screen": "0.81.0",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/stack": "^7.4.7",

    "crypto-js": "^4.2.0",
    "fs-extra": "^11.3.1",
    "i18next": "^25.3.6",
    "react": "19.1.0",
    "react-i18next": "^15.6.1",
    "react-native": "0.81.0",
    "react-native-calendar-events": "^2.2.0",
    "react-native-chart-kit": "^6.12.0",
    "react-native-device-info": "^14.0.4",
    "react-native-dotenv": "^3.4.11",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-get-random-values": "^1.11.0",
    "react-native-haptic-feedback": "^2.3.3",
    "react-native-image-picker": "^8.2.1",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-localize": "^3.5.2",
    "react-native-modal-datetime-picker": "^18.0.0",
    "react-native-paper": "^5.14.5",
    "react-native-permissions": "^5.4.2",
    "react-native-popup-menu": "^0.18.0",
    "react-native-purchases": "^9.2.1",
    "react-native-push-notification": "^8.1.1",
    "react-native-quick-crypto": "^0.7.17",
    "react-native-reanimated": "^4.0.2",
    "react-native-safe-area-context": "^5.5.2",
    "react-native-screens": "^4.14.1",
    "react-native-sensors": "^7.3.6",
    "react-native-share": "^12.2.0",
    "react-native-slider-x": "^0.11.3",
    "react-native-svg": "^15.12.1",
    "react-native-vector-icons": "^10.3.0",
    "react-native-video": "^6.16.1",
    "react-native-vision-camera": "^4.7.1",
    "react-native-wheel-color-picker": "^1.3.1",
    "react-native-worklets": "^0.4.1",
    "sharp": "^0.34.3",
    "twrnc": "^4.9.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "^20.0.0",
    "@react-native-community/cli-platform-android": "20.0.0",
    "@react-native-community/cli-platform-ios": "20.0.0",
    "@react-native/babel-preset": "0.81.0",
    "@react-native/codegen": "^0.81.0",
    "@react-native/eslint-config": "0.81.0",
    "@react-native/metro-config": "0.81.0",
    "@react-native/typescript-config": "0.81.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.1",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.1.0",
    "@types/react-native-push-notification": "^8.1.4",
    "@types/react-test-renderer": "^19.1.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-plugin-transform-inline-environment-variables": "^0.4.4",
    "eslint": "^8.19.0",
    "expo-camera": "^16.1.11",
    "husky": "^9.1.7",
    "jest": "^29.6.3",
    "lint-staged": "^16.1.5",
    "prettier": "2.8.8",
    "react-native-bundle-visualizer": "^3.1.3",
    "react-test-renderer": "19.1.0",
    "resolve.exports": "^2.0.3",
    "typescript": "^5.9.2"
  },
  "codegenConfig": {
    "name": "Nyth",
    "type": "modules",
    "jsSrcsDir": "specs",
    "android": {
      "javaPackageName": "com.nyth.specs",
      "modulesProvider": {
        "NativeAudioCaptureModule": "NativeAudioCaptureModuleProvider",
        "NativeAudioEffectsModule": "NativeAudioEffectsModuleProvider",
        "NativeAudioNoiseModule": "NativeAudioNoiseModuleProvider",
        "NativeAudioSafetyModule": "NativeAudioSafetyModuleProvider",
        "NativeAudioCoreModule": "NativeAudioCoreModuleProvider",
        "NativeAudioUtilsModule": "NativeAudioUtilsModuleProvider",
        "NativeAudioPipelineModule": "NativeAudioPipelineModuleProvider",
        "NativeAudioSpectrumModule": "NativeAudioSpectrumModuleProvider"
      }
    },
    "ios": {
      "modulesProvider": {
        "NativeAudioCaptureModule": "NativeAudioCaptureModuleProvider",
        "NativeAudioEffectsModule": "NativeAudioEffectsModuleProvider",
        "NativeAudioNoiseModule": "NativeAudioNoiseModuleProvider",
        "NativeAudioSafetyModule": "NativeAudioSafetyModuleProvider",
        "NativeAudioCoreModule": "NativeAudioCoreModuleProvider",
        "NativeAudioUtilsModule": "NativeAudioUtilsModuleProvider",
        "NativeAudioPipelineModule": "NativeAudioPipelineModuleProvider",
        "NativeAudioSpectrumModule": "NativeAudioSpectrumModuleProvider"
      }
    }
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
